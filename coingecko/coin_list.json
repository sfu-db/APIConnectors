{
    "version": 1,
    "request": {
        "url": "https://api.coingecko.com/api/v3/coins/list",
        "method": "GET",
        "params": {},
        "pagination": {
            "type": "offset",
            "offsetKey": "offset",
            "limitKey": "limit",
            "maxCount": 100
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.[*]",
        "schema": {
            "id": {
                "target": "$.id",
                "type": "string"
            },
            "symbol": {
                "target": "$.symbol",
                "type": "string"
            },
            "name": {
                "target": "$.name",
                "type": "string"
            }
        },
        "orient": "records"
    }
}