{
    "version": 1,
    "request": {
        "url": "https://api.currentsapi.services/v1/latest-news",
        "method": "GET",
        "authorization": {
            "type": "QueryParam",
            "keyParam": "apiKey"
        },
        "params": {
            "language": false
        },
        "pagination": {
            "type": "offset",
            "offsetKey": "offset",
            "limitKey": "limit",
            "maxCount": 100
        }
    },

    "response": {
        "ctype": "application/json",
        "tablePath": "$.news[*]",
        "schema": {
            "title": {
                "target": "$.title",
                "type": "string"
            },
            "category": {
                 "target": "$.category",
                "type": "list"
            },
            "description": {
                "target": "$.description",
                "type": "string"
            },
            "url": {
                "target": "$.url",
                "type": "string"
            },
            "author": {
                "target": "$.author",
                "type": "string"
            },
            "published": {
                "target": "$.published",
                "type": "string"
            }
        },
        "orient": "records"
    }
}
