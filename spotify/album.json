{
    "version": 1,
    "request": {
        "url": "https://api.spotify.com/v1/search?type=album",
        "method": "GET",
        "authorization": {
            "type": "OAuth2",
            "grantType": "ClientCredentials",
            "tokenServerUrl": "https://accounts.spotify.com/api/token"
        },
        "params": {
            "q": true,
            "limit": false,
            "offset": false
        },
        "pagination": {
            "type": "offset",
            "offsetKey": "offset",
            "limitKey": "limit",
            "maxCount": 50
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.albums.items[*]",
        "schema": {
            "album_name": {
                "target": "$.name",
                "type": "string"
            },
            "release_date": {
                "target": "$.release_date",
                "type": "string"
            },
            "total_tracks": {
                "target": "$.total_tracks",
                "type": "string"
            },
            "artist": {
                "target": "$.artists[*].name",
                "type": "object"
            },
            "available_markets": {
                "target": "$.available_markets",
                "type": "string"
            },
            "external urls": {
                "target": "$.external_urls",
                "type": "string"
            },
            "image url": {
                "target": "$.images[0].url",
                "type": "string"
            },
            "album type": {
                "target": "$.album_type",
                "type": "string"
            }
        },
        "orient": "records"
    }
}