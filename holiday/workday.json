{
    "version": 1,
    "request": {
        "url": "https://holidayapi.com/v1/workday",
        "method": "GET",
        "authorization": {
            "type": "QueryParam",
            "keyParam": "key"
        },
        "params": {
            "country": true,
            "start": true,
            "days": true
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.workday[*]",
        "schema": {
            "date": {
                "target": "$.date",
                "type": "string"
            },
            "weekday": {
                "target": "$.weekday.name",
                "type": "string"
            }
        },
        "orient": "records"
    }
}