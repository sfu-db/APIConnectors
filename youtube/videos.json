{
    "version": 1,
    "request": {
        "url": "https://www.googleapis.com/youtube/v3/search",
        "method": "GET",
        "authorization": {
            "type": "TokenParam",
            "keyParam": "key"
        },
        "params": {
            "q": true,
            "part": true,
            "type": false,
            "maxResults": false
        },
        "pagination":{
            "type":"cursor",
            "count_key":"maxResults",
            "max_count": 50
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.items[*]",
        "schema": {
            "etag":{
                "target": "$.etag",
                "type": "string"
            },
            "videoId":{
                "target": "$.id.videoId",
                "type": "string"
            },
            "publishedAt": {
                "target": "$.snippet.publishedAt",
                "type": "string"
            },
            "channelId": {
                "target": "$.snippet.channelId",
                "type": "string"
            },
            "title": {
                "target": "$.snippet.title",
                "type": "string"
            },
            "description": {
                "target": "$.snippet.description",
                "type": "string"
            },
            "channelTitle": {
                "target": "$.snippet.channelTitle",
                "type": "string"
            },
            "publishTime": {
                "target": "$.publishTime",
                "type": "string"
            }
        },
        "orient": "records"
    }
}